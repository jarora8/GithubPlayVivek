         DBU$RIB  DSECT=NO                                              00430000
         LTORG                                                          13130000
         END   CALLSQL                                                  13140000
         PUNCH ' SETCODE AC(0)'                                         13150000
         PUNCH ' SETSSI 04010000'                                       20840000
